<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp50"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/padding_left"
        android:paddingRight="@dimen/padding_right" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/height_item_menu"
            android:gravity="center_vertical"
            android:text="@string/fromDate"
            android:textSize="@dimen/text_size_content" />

        <EditText
            android:id="@+id/edt_from_date"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:clickable="true"
            android:cursorVisible="false"
            android:drawablePadding="2dp"
            android:drawableRight="@drawable/calendar_07"
            android:focusableInTouchMode="false"
            android:gravity="center_vertical"
            android:inputType="date"
            android:longClickable="false"
            android:text=""
            android:textSize="@dimen/text_size_content" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp50"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/padding_left"
        android:paddingRight="@dimen/padding_right" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/height_item_menu"
            android:gravity="center_vertical"
            android:text="@string/toDate"
            android:textSize="@dimen/text_size_content" />

        <EditText
            android:id="@+id/edt_to_date"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:clickable="true"
            android:cursorVisible="false"
            android:drawablePadding="2dp"
            android:drawableRight="@drawable/calendar_07"
            android:focusableInTouchMode="false"
            android:gravity="center_vertical"
            android:inputType="date"
            android:longClickable="false"
            android:text=""
            android:textSize="@dimen/text_size_content" />
    </LinearLayout>

    <Button
        android:id="@+id/imb_search_contract_payment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="@dimen/padding_left"
        android:layout_marginRight="@dimen/padding_right"
        android:layout_marginTop="10dp"

        android:filterTouchesWhenObscured="true"
        android:text="@string/report_out"
        android:textColor="#fff"
        android:textSize="@dimen/text_size_content" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@color/colorPrimary"
        android:gravity="center" >

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#00897b" />

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/nameStock"
            android:textColor="#fff" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/line_button_color" />

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/tv_quantity"
            android:textColor="#fff" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/line_button_color" />

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/discount"
            android:textColor="#fff" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/line_button_color" />

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/tientruocthue"
            android:textColor="#fff" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/line_button_color" />

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/tienthue"
            android:textColor="#fff" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/line_button_color" />

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/tv_amount"
            android:textColor="#fff" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#00897b" />
    </LinearLayout>

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/doi_tuong" />

</LinearLayout>