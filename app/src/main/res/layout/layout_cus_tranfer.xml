                            android:layout_height="wrap_content"
                            android:ems="5"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:text="@string/register_new_tv11_db" />

                        <EditText
                            android:id="@+id/edtMaGiayToDacBiet"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:layout_toRightOf="@+id/tvMaGiayToDacBiet"
                            android:gravity="center_vertical" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rlngaybatdau"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvNgayBd"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:ems="5"
                            android:text="@string/ngay_bd" />

                        <EditText
                            android:id="@+id/edtNgayBD"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:layout_toRightOf="@+id/tvNgayBd"
                            android:clickable="true"
                            android:drawablePadding="5dp"
                            android:drawableRight="@drawable/calendar_07"
                            android:focusableInTouchMode="false"
                            android:gravity="center_vertical" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rlngayketthuc"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvNgayKt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:ems="5"
                            android:layout_centerVertical="true"
                            android:text="@string/ngay_kt" />

                        <EditText
                            android:id="@+id/edtNgayKT"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:layout_toLeftOf="@+id/im_edtNgayKT"
                            android:layout_toRightOf="@+id/tvNgayKt"
                            android:clickable="true"
                            android:drawablePadding="5dp"
                            android:drawableRight="@drawable/calendar_07"
                            android:enabled="true"
                            android:focusableInTouchMode="false"
                            android:gravity="center_vertical" />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >



            <LinearLayout
                android:id="@+id/lnotpds"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal" >

                    <Button
                        android:id="@+id/btnSendOTP"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/button_height"
                        android:layout_gravity="right"
                        android:gravity="center_vertical"
                        android:background="@color/bg_tv_vt"
                        android:text="@string/sendOTP"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_content" >
                    </Button>

                    <ProgressBar
                        android:id="@+id/prbotp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lnotp"
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="5"
                        android:text="@string/sootp" />

                    <EditText
                        android:id="@+id/editotp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:inputType="text" />
                </LinearLayout>
            </LinearLayout>

            <include layout="@layout/layout_select_profile" />

            <LinearLayout
                android:id="@+id/lnhoso"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:visibility="gone"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvProfile"
                    android:layout_marginBottom="@dimen/margin_text_1"
                    android:layout_marginTop="@dimen/margin_text_1"
                    android:text="@string/thongtinhoso"

                    android:textColor="@color/list_background_pressed_vt"
                    android:textSize="@dimen/text_size_nomal"
                    android:textStyle="bold" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/view_size"
                    android:background="@color/color_view" />

                <ListView
                    android:id="@+id/lvUploadImage"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scrollbars="none" >
                </ListView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal" >

                <EditText
                    android:id="@+id/edt_capcha"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/height_spinner"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:hint="@string/captcha_hint"
                    android:inputType="phone"
                    android:maxLength="10"
                    android:singleLine="true"
                    android:textSize="@dimen/text_size_content" />

                <ImageView
                    android:id="@+id/imgCapcha"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5dp"
                    android:layout_weight="0.5"
                    android:scaleType="fitXY" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/lnDoituong"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:ems="6"
                    android:text="@string/doituong" />

                <Spinner
                    android:id="@+id/spnDoituong"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/button_height"
                    android:layout_gravity="right"
                    android:layout_marginLeft="2dp"
                    android:layout_weight="@integer/d4" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:ems="6"
                    android:text="@string/ngaykyCD" />

                <EditText
                    android:id="@+id/edit_ngayky"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginLeft="2dp"
                    android:layout_weight="@integer/d4"
                    android:clickable="true"
                    android:cursorVisible="false"
                    android:drawablePadding="5dp"
                    android:drawableRight="@drawable/calendar_07"
                    android:editable="false"
                    android:focusableInTouchMode="false"
                    android:longClickable="false"
                    android:maxLength="20" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/lnButton"
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_lin_layout_channel_manager"
                android:gravity="center"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/btndoisim"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/button_height"
                    android:layout_weight="1"
                    android:background="@color/bg_tv_vt"
                    android:text="@string/capnhat"
                    android:textColor="#fff"
                    android:textSize="@dimen/text_size_title" />

                <Button
                    android:id="@+id/btncancel"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/button_height"
                    android:layout_marginLeft="@dimen/button_margin"
                    android:layout_weight="1"

                    android:text="@string/cancel"
                    android:textColor="#fff"
                    android:textSize="@dimen/text_size_title"
                    android:visibility="gone" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/expanded_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible"
        android:layout_gravity="left|top" />
</FrameLayout>