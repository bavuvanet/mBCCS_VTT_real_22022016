<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/lntitle"
                android:layout_width="match_parent"
                android:layout_height="@dimen/button_height"
                android:background="@color/list_background_pressed_vt">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|left"
                    android:text="@string/servicemobile"
                    android:textColor="@color/white"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical"
                android:paddingLeft="@dimen/padding_left"
                android:paddingRight="@dimen/padding_right">

                <TextView
                    android:id="@+id/txtinfo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_text_1"
                    android:layout_marginTop="@dimen/margin_text_1"
                    android:text="@string/edit_customer_mobile"
                    android:textColor="@color/list_background_pressed_vt"
                    android:textSize="@dimen/text_size_14" />

                <View
                    android:id="@+id/viewhead"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/view_size"
                    android:background="@color/color_view" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:ems="7"
                        android:text="@string/loaikhmobile" />

                    <EditText
                        android:id="@+id/edtloaikh"

                        android:layout_width="0dp"
                        android:layout_height="@dimen/button_height"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="0.18"
                        android:clickable="true"
                        android:cursorVisible="false"
                        android:drawablePadding="2dp"
                        android:editable="false"
                        android:focusableInTouchMode="false"
                        android:gravity="center_vertical"
                        android:hint="Chọn loại KH"
                        android:longClickable="false"
                        android:text=""
                        android:textColor="@color/list_background_pressed_vt"
                        android:textSize="@dimen/text_size_content">

                        <requestFocus />
                    </EditText>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/tenkhang" />

                    <EditText
                        android:id="@+id/edit_tenKH"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:maxLength="120">

                        <requestFocus />

                    </EditText>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/ngaysinh_mobile" />

                    <EditText
                        android:id="@+id/edit_ngaysinh"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:clickable="true"
                        android:cursorVisible="false"
                        android:drawablePadding="5dp"
                        android:drawableRight="@drawable/calendar_07"
                        android:editable="false"
                        android:focusableInTouchMode="false"
                        android:longClickable="false"
                        android:maxLength="20" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ln_sex"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/sexMobile" />

                    <Spinner
                        android:id="@+id/spinner_gioitinh"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/button_height"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lnsogiayto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/typegiayto" />

                    <Spinner
                        android:id="@+id/spinner_type_giay_to"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/button_height"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4" />

                    <ProgressBar
                        android:id="@+id/prbtypePaperEdit"
                        android:layout_width="25dp"
                        android:layout_height="wrap_content"
                        android:visibility="visible" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearCMT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/so_cmnd_mobile" />

                    <EditText
                        android:id="@+id/edit_socmnd"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:digits="0123456789"
                        android:maxLength="12"
                        android:numeric="integer">

                        <requestFocus />
                    </EditText>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearsoGPKD"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="@integer/d5"
                        android:text="@string/soGPKDorQDTL" />

                    <EditText
                        android:id="@+id/edit_soGQ"

                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:maxLength="200">

                        <requestFocus />
                    </EditText>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ln_tin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/tax_code" />

                    <EditText
                        android:id="@+id/edt_tin"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:inputType="text"
                        android:maxLength="200">

                        <requestFocus />
                    </EditText>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lnngaycapnew"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/ngaycap_mobile" />

                    <EditText
                        android:id="@+id/edit_ngaycap"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:clickable="true"
                        android:cursorVisible="false"
                        android:drawablePadding="5dp"
                        android:drawableRight="@drawable/calendar_07"
                        android:editable="false"
                        android:focusableInTouchMode="false"
                        android:longClickable="false"
                        android:maxLength="20" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lnngayhethan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="@integer/d5"
                        android:text="@string/ngayhethan" />

                    <EditText
                        android:id="@+id/edit_ngayhethan"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:clickable="true"
                        android:cursorVisible="false"
                        android:drawablePadding="5dp"
                        android:drawableRight="@drawable/calendar_07"
                        android:editable="false"
                        android:focusableInTouchMode="false"
                        android:longClickable="false"
                        android:maxLength="20" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/lnquoctichdialog"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/tv_quoc_tich" />

                    <Spinner
                        android:id="@+id/spinner_quoctichnew"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/button_height"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/lnnoicapnew"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/noicap_mobile" />

                    <EditText
                        android:id="@+id/edit_noicap"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:maxLength="100">

                        <requestFocus />
                    </EditText>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/lnAddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:ems="7"
                            android:text="@string/provinceMobile" />

                        <EditText
                            android:id="@+id/edtprovince"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/button_height"
                            android:layout_gravity="right"
                            android:layout_marginLeft="2dp"
                            android:layout_weight="@integer/d4"
                            android:clickable="true"
                            android:cursorVisible="false"
                            android:drawablePadding="2dp"
                            android:editable="false"
                            android:focusableInTouchMode="false"
                            android:gravity="center_vertical"
                            android:hint="Chọn Tỉnh/TP"
                            android:longClickable="false"
                            android:text=""
                            android:textColor="@color/list_background_pressed_vt"
                            android:textSize="@dimen/text_size_content"></EditText>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:ems="7"
                            android:text="@string/districtMobile" />

                        <EditText
                            android:id="@+id/edtdistrict"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/button_height"
                            android:layout_gravity="right"
                            android:layout_marginLeft="2dp"
                            android:layout_weight="@integer/d4"
                            android:clickable="true"
                            android:cursorVisible="false"
                            android:drawablePadding="2dp"
                            android:editable="false"
                            android:focusableInTouchMode="false"
                            android:gravity="center_vertical"
                            android:hint="Chọn Quận/Huyện"
                            android:longClickable="false"
                            android:text=""
                            android:textColor="@color/list_background_pressed_vt"
                            android:textSize="@dimen/text_size_content"></EditText>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:ems="7"
                            android:text="@string/precintMobile" />

                        <EditText
                            android:id="@+id/edtprecinct"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/button_height"
                            android:layout_gravity="right"
                            android:layout_marginLeft="2dp"
                            android:layout_weight="@integer/d4"
                            android:clickable="true"
                            android:cursorVisible="false"
                            android:drawablePadding="2dp"
                            android:editable="false"
                            android:focusableInTouchMode="false"
                            android:gravity="center_vertical"
                            android:hint="Chọn Phường/Xã"
                            android:longClickable="false"
                            android:text=""
                            android:textColor="@color/list_background_pressed_vt"
                            android:textSize="@dimen/text_size_content"></EditText>
                    </LinearLayout>

                    <!-- Them moi ToThong, Duong dang code -->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:ems="7"
                            android:text="@string/tv_to_thon_creat_address_no_required" />

                        <EditText
                            android:id="@+id/edtStreetBlock"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/button_height"
                            android:layout_gravity="right"
                            android:layout_marginLeft="2dp"
                            android:layout_weight="@integer/d4"
                            android:clickable="true"
                            android:cursorVisible="false"
                            android:drawablePadding="2dp"
                            android:editable="false"
                            android:focusableInTouchMode="false"
                            android:gravity="center_vertical"
                            android:hint="Chọn Tổ/Thôn"
                            android:longClickable="false"
                            android:text=""
                            android:textColor="@color/list_background_pressed_vt"
                            android:textSize="@dimen/text_size_content"></EditText>

                        <Button
                            android:id="@+id/btnRefreshStreetBlock"
                            android:layout_width="35dp"
                            android:layout_height="35dp"
                            android:layout_marginLeft="2dp"
                            android:background="@drawable/ic_menu_refresh"
                            android:visibility="gone" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:ems="7"
                            android:gravity="center_vertical"
                            android:text="@string/duongConnect"
                            />

                        <EditText
                            android:id="@+id/edt_streetName"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/button_height"
                            android:layout_gravity="right"
                            android:layout_marginLeft="2dp"
                            android:layout_weight="@integer/d4"
                            android:drawablePadding="2dp"
                            android:focusableInTouchMode="true"
                            android:gravity="center_vertical"
                            android:hint="Nhập tên đường"
                            android:text=""
                            android:textColor="@color/list_background_pressed_vt"
                            android:textSize="@dimen/text_size_content"></EditText>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        android:weightSum="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:ems="7"
                            android:gravity="center_vertical"
                            android:text="@string/sonhaConnect"
                            />

                        <EditText
                            android:id="@+id/edtHomeNumber"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/button_height"
                            android:layout_gravity="right"
                            android:layout_marginLeft="2dp"
                            android:layout_weight="1.01"
                            android:drawablePadding="2dp"
                            android:focusableInTouchMode="true"
                            android:gravity="center_vertical"
                            android:hint="Nhập số nhà"
                            android:text=""
                            android:textColor="@color/list_background_pressed_vt"
                            android:textSize="@dimen/text_size_content"></EditText>
                    </LinearLayout>



                </LinearLayout>






                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ems="7"
                        android:text="@string/note" />

                    <EditText
                        android:id="@+id/edit_note"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:maxLength="120">

                        <requestFocus />
                    </EditText>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="@integer/d5"
                        android:gravity="center_vertical"
                        android:text="@string/reruire_reason"
                        android:textSize="@dimen/text_size_content" />

                    <Spinner
                        android:id="@+id/spn_reason"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/button_height"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4"
                        android:drawablePadding="2dp"
                        android:focusableInTouchMode="true"
                        android:gravity="center_vertical"
                        android:textColor="@color/list_background_pressed_vt"
                        android:textSize="@dimen/text_size_content"></Spinner>

                    <ProgressBar
                        android:id="@+id/prbreason"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="gone"/>
                </LinearLayout>

                <include layout="@layout/layout_select_profile" />


                <LinearLayout
                    android:id="@+id/lnhoso"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:visibility="gone"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/tvProfile"
                        android:layout_marginBottom="@dimen/margin_text_1"
                        android:layout_marginTop="@dimen/margin_text_1"
                        android:text="@string/thongtinhoso"
                        android:textColor="@color/list_background_pressed_vt"
                        android:textSize="@dimen/text_size_nomal"
                        android:textStyle="bold" />

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/view_size"
                        android:background="@color/color_view" />

                    <ListView
                        android:id="@+id/lvUploadImage"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scrollbars="none" >
                    </ListView>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/lnDoituong"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/width_register_info"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="@string/doituong" />

                    <Spinner
                        android:id="@+id/spnDoituong"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/button_height"
                        android:layout_gravity="right"
                        android:layout_marginLeft="2dp"
                        android:layout_weight="@integer/d4" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lnotpChangeCus"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/lnOTP"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="@integer/d5"
                                android:gravity="center_vertical"
                                android:text="@string/edtOTPIsdn"
                                android:textSize="@dimen/text_size_content" />

                            <EditText
                                android:id="@+id/edtOTPIsdn"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/button_height"
                                android:layout_gravity="right"
                                android:layout_marginLeft="2dp"
                                android:layout_weight="@integer/d4"
                                android:drawablePadding="2dp"
                                android:focusableInTouchMode="true"
                                android:gravity="center_vertical"
                                android:hint="Số điện thoại khách hàng"
                                android:inputType="phone"
                                android:maxLength="11"
                                android:textColor="@color/list_background_pressed_vt"
                                android:textSize="@dimen/text_size_content"></EditText>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/btnSendOTP"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/button_height"
                                android:layout_gravity="right"
                                android:layout_marginLeft="2dp"
                                android:drawablePadding="2dp"
                                android:gravity="center_vertical"
                                android:text="@string/sendOTP"
                                android:textColor="@color/white"
                                android:textSize="@dimen/text_size_content"></Button>

                            <ProgressBar
                                android:id="@+id/prbreasonBtn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:visibility="gone" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="@integer/d5"
                                android:gravity="center_vertical"
                                android:text="@string/edtOTPCode"
                                android:textSize="@dimen/text_size_content" />

                            <EditText
                                android:id="@+id/edtOTPCode"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/button_height"
                                android:layout_gravity="right"
                                android:layout_marginLeft="2dp"
                                android:layout_weight="@integer/d4"
                                android:drawablePadding="2dp"
                                android:focusableInTouchMode="true"
                                android:gravity="center_vertical"
                                android:hint="Mã xác thực gửi cho khách hàng"
                                android:inputType="numberSigned"
                                android:textColor="@color/list_background_pressed_vt"
                                android:textSize="@dimen/text_size_content"></EditText>
                        </LinearLayout>
                    </LinearLayout>


                </LinearLayout>


                <LinearLayout
                    android:id="@+id/lnButton"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_lin_layout_channel_manager"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/btnsuadoi"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/button_height"
                        android:layout_weight="1"
                        android:background="@drawable/round_button"
                        android:text="@string/suadoi"
                        android:textColor="#fff"
                        android:textSize="@dimen/text_size_title" />

                    <Button
                        android:id="@+id/btncancel"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/button_height"
                        android:layout_marginLeft="@dimen/button_margin"
                        android:layout_weight="1"
                        android:background="@drawable/round_button"
                        android:text="@string/cancel"
                        android:textColor="#fff"
                        android:textSize="@dimen/text_size_title" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/expanded_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />
</FrameLayout>