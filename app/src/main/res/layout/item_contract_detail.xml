<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/activity_vertical_margin"
    android:layout_marginRight="@dimen/activity_vertical_margin"
    android:background="@color/background"

    android:orientation="vertical">
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#FDFEFE" />
    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#FDFEFE" />


        <TextView
            android:id="@+id/txtContractNo"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="14"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="@dimen/text_size_13_5" />

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#FDFEFE" />

        <TextView
            android:id="@+id/txtDate"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="@dimen/text_size_13_5" />

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#FDFEFE" />


        <!--<CheckBox-->
            <!--android:id="@+id/imContract"-->
            <!--android:layout_width="0dp"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_gravity="center"-->
            <!--android:layout_weight="3"-->
            <!--android:focusable="false"-->
            <!--android:focusableInTouchMode="false"-->
            <!--android:gravity="center"-->
            <!--android:textSize="@dimen/text_size_13_5" />-->

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#FDFEFE" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#FDFEFE" />
    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/notifi_choose"
            android:layout_width="match_parent"
            android:text="@string/notify_choose_sub"
            android:textColor="@color/textDialogTitle"
            android:layout_height="wrap_content"
             />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="10"
            android:gravity="center_vertical"
            android:text="@string/sothuebao"
            android:textSize="@dimen/text_size_13_5"
            android:textStyle="bold" />

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#FDFEFE" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:gravity="center_vertical"
            android:text="@string/dichvu"
            android:textSize="@dimen/text_size_13_5"
            android:textStyle="bold" />

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#FDFEFE" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:gravity="center_vertical"
            android:text="@string/liquidationInfo"
            android:textSize="@dimen/text_size_13_5"
            android:textStyle="bold" />

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#FDFEFE" />


        <TextView
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_weight="0"
            android:gravity="center_vertical"
            android:text="@string/chon"
            android:textSize="@dimen/text_size_13_5"
            android:textStyle="bold" />

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#FDFEFE" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#FDFEFE" />
    </TableRow>
</LinearLayout>

